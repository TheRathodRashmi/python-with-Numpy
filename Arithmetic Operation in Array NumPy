# 1. Addition (+)
import numpy as np
x=np.array([[1,2],
            [3,4]])
y=np.array([[11,12],
            [13,14]])

addition= x+y
print(addition)
print("--------------------------------------------")
# 2. Substraction (-)
import numpy as np
x=np.array([[1,2],
            [3,4]])
y=np.array([[11,12],
            [13,14]])

substraction= x-y
print(substraction)
print("--------------------------------------------")
substraction= y-x
print(substraction)
print("---------------------------------------------------------")
# 3. Multiplication (*)
import numpy as np
x=np.array([[1,2],
            [3,4]])
y=np.array([[11,12],
            [13,14]])

Multiplication= x*y
print(Multiplication)
print("---------------------------------------------------------")
# 4. Matrix Multiplication (@)
import numpy as np
x=np.array([[1,2],
            [3,4]])
y=np.array([[11,12],
            [13,14]])

MatrixMultiplication= x @ y
print(MatrixMultiplication)

print("---------------------------------------------------------")
# 5.Division (/)
import numpy as np
x=np.array([[1,2],
            [3,4]])
y=np.array([[11,12],
            [13,14]])

division= x / y
print(division)

print("---------------------------------------------------------")
# 6. Floor Division (//)
import numpy as np
x=np.array([[1,2],
            [3,4]])
y=np.array([[11,12],
            [13,14]])

floor_division= x // y
print(floor_division)
print("---------------------------------------------------------")
# 7. Exponentiation (**)
import numpy as np
x=np.array([[1,2],
            [3,4]])
y=np.array([[11,12],
            [13,14]])

expo= y ** x
print(expo)
print("---------------------------------------------------------")
# 8. Modulo (%)
import numpy as np
x=np.array([[1,2],
            [3,4]])
y=np.array([[11,12],
            [13,14]])

modulo= y % x
print(modulo)

print("---------------------------------------------------------")
# 9. Transpose 
import numpy as np
x=np.array([[1,2],
            [3,4]])
y=np.array([[11,12],
            [13,14]])

print(x.transpose())
print(y.transpose())

